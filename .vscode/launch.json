{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug batch.main (WSL)",
      "type": "python",
      "request": "launch",

      "module": "batch.main",
      "cwd": "${workspaceFolder}",

      "console": "integratedTerminal",

      "justMyCode": false,
      "subProcess": true,

      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "PYTHONWARNINGS": "ignore::UserWarning:pkg_resources"
      },

      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
